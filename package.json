{
  "name": "travis-builds-reporter",
  "version": "1.0.0",
  "description": "A Travis CI CLI tool that returns basic builds statistics for a Travis enabled GitHub *public* repository.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "lint": "./node_modules/.bin/eslint ./index.js ./src/TravisBuildsUtils.js ./test/test.js ./test/TravisBuildsUtilsTest.js",
    "test": "mocha",
    "coverage": "./node_modules/.bin/istanbul cover ./index.js -- -r niktekusho/travis-builds-reporter",
    "depcheck": "./node_modules/.bin/depcheck ."
  },
  "author": {
    "name": "Nicola Dal Maso",
    "url": "https://github.com/niktekusho/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/niktekusho/travis-builds-reporter.git"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "axios": "^0.15.3",
    "commander": "^2.9.0",
    "prompt": "^1.0.0"
  },
  "devDependencies": {
    "depcheck": "^0.6.7",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "chai": "*"
  }
}
