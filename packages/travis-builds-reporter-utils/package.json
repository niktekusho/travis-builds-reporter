{
  "name": "travis-builds-reporter-utils",
  "version": "0.0.2",
  "description": "Utilities (stats calculators basically) of the travis-builds-reporter project",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "alldocs": "npm run doc && npm run doc2md",
    "coverage": "nyc --reporter=html --reporter=lcov --reporter=text mocha --recursive",
    "doc": "jsdoc src -r --debug -d docs",
    "doc2md": "jsdoc2md src/**/*.js > docs/api.md",
    "lint": "eslint src/**/*.js test/**/*.js",
    "prepublish": "babel src --out-dir dist",
    "test": "mocha --recursive"
  },
  "keywords": [
    "travis-builds-reporter"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/niktekusho/travis-builds-reporter.git"
  },
  "author": {
    "name": "Nicola Dal Maso",
    "url": "https://github.com/niktekusho/"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.0.2",
    "eslint": "^4.1.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.1",
    "jsdoc": "^3.5.3",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3"
  },
  "engines": {
    "node": ">=7.0.0"
  }
}
